# LLM settings
OPENAI_API_KEY=
MODEL=

# Safety mode (do not change in shared environments)
KUBECLAW_MODE=read_only

# Cluster target (lab-only defaults)
KUBE_CONTEXT=kind-kubeclaw
KUBE_NAMESPACE=demo
KUBECLAW_CMD_TIMEOUT_SECONDS=10
KUBECLAW_MAX_TOOL_CALLS=10

# Dev-only override for wrong-cluster server tripwire (default OFF)
KUBECLAW_ALLOW_UNSAFE_CLUSTER=false

# Reporting output
REPORTS_DIR=reports
SNAPSHOTS_DIR=snapshots
